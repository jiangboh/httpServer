<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
	<connectionStrings>
		<add name="dbConStr" connectionString="Server=172.17.8.130;Database=hsdatabase;UserID=root;Password=root"/>
	</connectionStrings>

	<appSettings>
		<!-- Log打印级别,由低到高分别为：debug、info、warning、error、nowrite  -->
		<add key="logLevel" value="debug" />
		<!-- Log最大字节数，单位字节  -->
		<add key="MaxLogFileSize" value="1000000" />
		
		<!-- HTTP SERVER 的IP地址  -->
		<add key="HttpServerIp" value="192..168.88.101" />
		<!-- HTTP SERVER 的用户名  -->
		<add key="HttpServerName" value="name" />
		<!-- HTTP SERVER 的密码  -->
		<add key="HttpServerPasswd" value="passwd" />
		
		<!-- NTP SERVER 的地址  -->
		<add key="Ntp1ServerPath" value="173.21.21.1" />
		<add key="Ntp2ServerPath" value="173.21.21.1" />
		
		<!-- 反向连接AP的用户名和密码  -->
		<add key="ConnectionRequestUsername" value="name" />
		<add key="ConnectionRequestPassWd" value="passwd" />
		
		<!-- HTTP 文件服务器根在本地的路径  -->
		<add key="UploadServerRootPath" value="d:\\" />
		<!-- HTTP 文件服务器的Url  -->
		<add key="UploadServerUrl" value="http://173.21.21.1:123" />
		<!-- HTTP 文件服务器的用户名  -->
		<add key="UploadServerUser" value="uploadName" />
		<!-- HTTP 文件服务器的密码  -->
		<add key="UploadServerPasswd" value="uploadPasswd" />
         
		<!-- 多长时间（分钟）未收到AP心跳，认为AP离线  -->
		<add key="ApOffLineTime" value="4" />
		<!-- TR069根节点（"Device."或者"InternetGatewayDevice."）  -->
		<add key="XmlRootNode" value="Device." />

	</appSettings>
</configuration>